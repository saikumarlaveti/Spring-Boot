package com.results.beans;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class Lokesh {
	
	public Saikumar ref;
	@Autowired
	Lokesh(Saikumar ref){
		this.ref  = ref;
	}
	
	public void wish() {
		System.out.println("Hello Saikumar");
		ref.greet("This is Saikumar Laveti");
	}

}
